# ClickHouse Cloud
CLICKHOUSE_HOST=xxx.clickhouse.cloud
CLICKHOUSE_PORT=8443
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=xxx
CLICKHOUSE_DATABASE=uptime_monitor

# Grafana Cloud (OpenTelemetry)
OTEL_EXPORTER_OTLP_ENDPOINT=https://otlp-gateway-prod-us-central-0.grafana.net/otlp
GRAFANA_CLOUD_INSTANCE_ID=xxx
GRAFANA_CLOUD_TOKEN=xxx

# Target to monitor
TARGET_URL=https://globaldial.co
TARGET_LOGIN_URL=https://globaldial.co/login
TARGET_EMAIL=your-test-account@email.com
TARGET_PASSWORD=your-test-password

# Check intervals (in minutes)
HTTP_CHECK_INTERVAL=1
BROWSER_CHECK_INTERVAL=5

# Server
PORT=3000
NODE_ENV=production